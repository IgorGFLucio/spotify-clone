    "build:prod": "export BUILD_STATIC=true && env-cmd -f env/.env.production yarn build:static",
    "start": "next start",
    "lint": "next lint",
    "prepare": "husky install"
  },
  "lint-staged": {
    "./src/**/*.{ts,js,jsx,tsx}": []
  },
  "dependencies": {
    "@chakra-ui/react": "2.2.1",
    "@emotion/react": "11.9.3",
    "@emotion/styled": "11.9.3",
    "axios": "^0.27.2",
    "date-fns": "^2.29.1",
    "dotenv": "^16.0.0",
    "env-cmd": "^10.1.0",
    "framer-motion": "^6",
    "i18next": "^21.8.12",
    "next": "12.1.6",
    "react": "18.2.0",
    "react-animate-on-scroll": "^2.1.5",
    "react-dom": "18.2.0",
    "react-hook-form": "^7.32.1",
    "react-i18next": "^11.17.4",
    "react-multi-carousel": "^2.8.1",
    "react-query": "^3.39.1",
    "react-scroll": "^1.8.7",
