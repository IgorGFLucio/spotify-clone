{
  "name": "spotify-clone",
  "description": "spotify-clone",
  "version": "1.0.0",
  "private": true,
  "engines": {
    "node": ">=14.x",
    "npm": "You must use yarn in this project!",
    "yarn": "^1.22.x"
  },
  "scripts": {
    "dev": "next",
    "build": "next build",
    "build:static": "next build && next export",
