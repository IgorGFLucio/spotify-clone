    "@typescript-eslint/parser": "5.28.0",
    "autoprefixer": "^10.4.7",
    "babel-plugin-styled-components": "2.0.7",
    "eslint": "8.18.0",
    "eslint-config-next": "12.1.6",
    "eslint-config-prettier": "8.5.0",
    "eslint-import-resolver-typescript": "2.7.1",
    "eslint-plugin-import": "2.26.0",
