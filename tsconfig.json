{
  "compilerOptions": {
    "target": "ES2020",
    "lib": ["dom", "dom.iterable", "esnext", "ES2020"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": false,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "baseUrl": "./src",
    "paths": {
      "@global-layouts/*": ["./layouts/*"],
      "@global-components/*": ["./components/*"],
      "@global-utils/*": ["./utils/*"],
      "@global-hooks/*": ["./hooks/*"],
      "@global-stores/*": ["./stores/*"],
      "@global-libs/*": ["./libs/*"],
      "@pages/*": ["./pages/*"],
      "@entities/*": ["./entities/*"],
      "@services/*": ["./services/*"],
      "@services": ["./services"],
      "@styles": ["./styles"],
      "@styles/*": ["./styles/*"],
      "@configs/*": ["./configs/*"],
      "@assets/*": ["../public/assets/*"],
      "@fonts/*": ["../public/fonts/*"],
      "@translations": ["./translations"],
      "@home/*": ["./modules/home/*"],
      "@meetingRoom/*": ["./modules/meetingRoom/*"],
      "@diningRoom/*": ["./modules/diningRoom/*"],
      "@workRoom/*": ["./modules/workRoom/*"],
      "@workspace/*": ["./modules/workspace/*"],
      "@ranking/*": ["./modules/ranking/*"],
      "@exit/*": ["./modules/exit/*"]
    },
    "incremental": true
  },
  "exclude": ["node_modules"],
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", "src/*"]
}
